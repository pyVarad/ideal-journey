{
  "name": "design-system-impl",
  "version": "0.0.2",
  "description": "Design System",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/angular",
    "packages/stencil",
    "packages/react",
    "packages/documentation/react",
    "packages/documentation/sb-angular"
  ],
  "scripts": {
    "clean": "ts-node ./bin/clean.ts",
    "build": "yarn build:stencil && yarn build:angular && yarn build:react",
    "lint": "yarn lint:stencil",
    "build:stencil": "yarn --cwd packages/stencil build",
    "build:angular": "yarn --cwd packages/angular build",
    "build:react": "yarn --cwd packages/react build",
    "lint:stencil": "yarn --cwd packages/stencil lint",
    "start": "yarn --cwd packages/stencil start",
    "docs:sb:react": "yarn --cwd packages/documentation/react storybook",
    "docs:sb:ng": "yarn --cwd packages/documentation/sb-angular storybook"
  },
  "keywords": [
    "StencilJS"
  ],
  "author": "Varad A G",
  "engines": {
    "node": ">=18.6.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "typescript-logging": "^2.1.0",
    "typescript-logging-log4ts-style": "^2.1.0",
    "@storybook/angular": "^7.5.3",
    "@angular-devkit/build-angular": "16.1.6",
    "@babel/core": "^7.23.3",
    "@babel/generator": "^7.23.4",
    "webpack": "^5.89.0",
    "autoprefixer": "^10.4.16",
    "pacote": "^17.0.4",
    "@angular/cli": "^17.0.3",
    "cacache": "^18.0.0",
    "npm-pick-manifest": "9.0.0",
    "jsdom": "23.0.0",
    "jest-environment-jsdom": "29.7.0",
    "globby": "14.0.0",
    "semver": "7.5.4"
  }
}